<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PainReliefExcersises"
        tools:targetApi="31">
        <activity
            android:name=".KneePain"
            android:exported="false" />
        <activity
            android:name=".HipPain"
            android:exported="false" />
        <activity
            android:name=".GroinPain"
            android:exported="false" />
        <activity
            android:name=".ElbowPain"
            android:exported="false" />
        <activity
            android:name=".WristPain"
            android:exported="false" />
        <activity
            android:name=".ShoulderPain"
            android:exported="false" />
        <activity
            android:name=".NeckPain"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".BackPain"
            android:parentActivityName=".MainActivity" />
    </application>

</manifest>